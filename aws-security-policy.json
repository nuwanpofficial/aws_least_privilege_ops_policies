				"trustedadvisor:IncludeCheckItems",
				"waf:Create*",
				"waf:Delete*",
				"waf:Put*",
				"waf:Update*",
				"waf:TagResource",
				"waf:UntagResource",
				"waf-regional:Create*",
				"waf-regional:Delete*",
				"waf-regional:Update*",
				"waf-regional:Put*",
