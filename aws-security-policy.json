{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "IAM",
            "Effect": "Allow",
            "Action": [
                "iam:PassRole",
                "iam:Get*",
                "iam:List*",
                "access-analyzer:ValidatePolicy",
                "access-analyzer:TagResource",
                "access-analyzer:CreateAnalyzer",
                "access-analyzer:DeleteAnalyzer",
                "access-analyzer:StartPolicyGeneration",
                "sts:Get*"
            ],
            "Resource": [
                "*"
            ]
        },
        {
