				"backup:Cancel*",
				"backup:Copy*",
				"backup:Create*",
				"backup:Delete*",
				"backup:Disassociate*",
				"backup:Put*",
				"backup:Start*",
				"backup:Stop*",
				"backup:Update*",
				"backup:TagResource",
				"backup:UntagResource",
				"kms:Decrypt",
